{
  "name": "carrier",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "start": "concurrently --kill-others \"electron-vite\" \"vite\"",
    "prebuild": "electron-vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "commitlint": "commitlint --config commitlint.config.ts",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "formats": "prettier --cache --write .",
    "lints": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy:osx": "electron-builder --mac",
    "deploy:win": "concurrently \"pnpm run deploy:win32\" \"pnpm run deploy:win64\"",
    "deploy:win32": "npx electron-builder --win nsis:ia32",
    "deploy:win64": "npx electron-builder --win nsis:x64"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@vanilla-extract/css": "^1.17.0",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/vite-plugin": "^4.0.19",
    "axios": "^1.7.9",
    "emoji-picker-react": "^4.12.0",
    "jotai": "^2.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.1.2",
    "react-spinners": "^0.15.0",
    "react-use": "^17.6.0",
    "swiper": "^11.2.5",
    "vite": "5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.1.2",
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^2.3.0",
    "eslint": "^9.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2"
  },
  "packageManager": "pnpm@9.15.4+sha256.9bee59c7313a216722c079c1e22160dea7f88df4e0c3450b1d7b01b882336c6a"
}
